  <process id="InitialServiceOrderAckProcess" name="Initial Service Order Acknowledge Process" isExecutable="true">
    <userTask id="userTaskAckServiceOrder" name="Acknowledge Service Order" activiti:candidateGroups="admin"></userTask>
    <startEvent id="startevent1" name="Start"></startEvent>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="userTaskAckServiceOrder"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow2" sourceRef="userTaskAckServiceOrder" targetRef="exclusivegateway1"></sequenceFlow>
    <serviceTask id="servicetaskReject" name="Reject Service" activiti:delegateExpression="${rejectServiceOrderBean}"></serviceTask>
    <sequenceFlow id="flowRejected" name="Rejected" sourceRef="exclusivegateway1" targetRef="servicetaskReject">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!approved}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <serviceTask id="serviceTaskAddToProcessScheduler" name="Add Service Order to Process Scheduler" activiti:delegateExpression="${addServiceOrderToScheduler}"></serviceTask>
    <sequenceFlow id="flowApproved" name="Approved" sourceRef="exclusivegateway1" targetRef="serviceTaskAddToProcessScheduler">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approved}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow5" sourceRef="serviceTaskAddToProcessScheduler" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="servicetaskReject" targetRef="endevent1"></sequenceFlow>
    <textAnnotation id="textannotation1">
      <text>ServiceOrder.status = Initial Service= Feasibility Checked (no entry in Inventory)
User modifies in the Form
- startDate,
- expectedCompletionDate
			</text>
    </textAnnotation>
    <association id="association1" sourceRef="textannotation1" targetRef="usertask2" associationDirection="None"></association>
    <textAnnotation id="textannotation2">
      <text>ServiceOrder.status = InProgress
				Service.state = Inactive (Creates entry in Inventory)
			</text>
    </textAnnotation>
    <association id="association2" sourceRef="textannotation2" targetRef="servicetask3" associationDirection="None"></association>
    <association id="association3" sourceRef="textannotation1" targetRef="userTaskAckServiceOrder" associationDirection="None"></association>
    <association id="association4" sourceRef="textannotation2" targetRef="serviceTaskAddToProcessScheduler" associationDirection="None"></association>
  </process>
